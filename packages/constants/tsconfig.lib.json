{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"declaration": true,
		"types": ["node", "undici-types"],
		"target": "es2021",
		"strictNullChecks": false,
		"noImplicitAny": false,
		"strictBindCallApply": false,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": false
	},
	"include": ["src"],
	"exclude": ["dist", "node_modules"]
}
